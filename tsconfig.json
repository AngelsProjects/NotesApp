{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": true,
    "alwaysStrict": true,
    "baseUrl": "./",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "inlineSources": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "ES5", "ES6", "ES7", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "build",
    "paths": {
      "@actions/*": ["src/actions/*"],
      "@actions": ["src/actions"],
      "@assets/*": ["src/assets/*"],
      "@assets": ["src/assets"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@constants/*": ["src/constants/*"],
      "@constants": ["src/constants"],
      "@helpers/*": ["src/helpers/*"],
      "@helpers": ["src/helpers"],
      "@layouts/*": ["src/layouts/*"],
      "@layouts": ["src/layouts"],
      "@interfaces/*": ["src/interfaces/*"],
      "@interfaces": ["src/interfaces"],
      "@reducers/*": ["src/reducers/*"],
      "@reducers": ["src/reducers"],
      "@providers/*": ["src/providers/*"],
      "@providers": ["src/providers"],
      "@services/*": ["src/services/*"],
      "@services": ["src/services"],
      "@styles/*": ["src/styles/*"],
      "@styles": ["src/styles"],
      "@views/*": ["src/views/*"],
      "@views": ["src/views"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"]
    },
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext"
  },
  "exclude": [
    "build",
    "dist",
    "node_modules",
    ".webpack",
    "_warmup",
    ".vscode"
  ],
  "include": ["src/**/*.ts"],
  "types": ["src/types", "typePatches", "types"],
  "typeRoots": ["src/types", "node_modules/@types","types"]
}
