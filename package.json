  "version": "0.1.0",
  "scripts": {
    "build": "yarn build:dev",
    "build:dev": "yarn tailwind && NODE_ENV=development webpack",
    "build:prod": "yarn tailwind && NODE_ENV=production webpack",
    "deploy": "yarn deploy:dev",
    "deploy:dev": "yarn tailwind && NODE_ENV=development deploy=true webpack",
    "deploy:prod": "yarn tailwind && NODE_ENV=production deploy=true webpack",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf yarn.lock && yarn",
    "lint": "eslint .",
    "start": "yarn start:dev",
    "start:dev": "yarn tailwind && NODE_ENV=development webpack-dev-server --open --hot",
    "start:local": "yarn tailwind && NODE_ENV=local webpack-dev-server --open --hot",
    "start:prod": "yarn tailwind && NODE_ENV=production webpack-dev-server --open --hot",
    "tailwind": "tailwind build ./src/styles/tailwind/config.scss -c ./tailwind.config.js -o ./src/styles/tailwind/core.scss",
    "test": "echo 'test'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
